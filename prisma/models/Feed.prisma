model Feed {
    id          String    @id @default(uuid())
    title       String
    link        String
    image       String?
    description String?
    pubDate     DateTime?
    author      String?
    domain      String?
    userId      String?
    createdAt   DateTime  @default(now())
    updatedAt   DateTime  @updatedAt
    user        User?     @relation(fields: [userId], references: [id])
}
