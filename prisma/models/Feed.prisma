model Feed {
    id          String       @id @default(uuid())
    title       String
    link        String
    image       String?
    description String?
    pubDate     DateTime?
    author      String?
    domain      String?
    userId      String?
    rssId       String
    createdAt   DateTime     @default(now())
    updatedAt   DateTime     @updatedAt
    user        User?        @relation(fields: [userId], references: [id])
    rss         RssGenerator @relation(fields: [rssId], references: [id])
}
