model RssGenerator{
    id        String   @id @default(uuid())
    type      String
    website   String
    rssUrl    String?
    link      String?
    selector  String?
    userId    String?
    lastUpdate DateTime?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    tasks     ExecuteTask[]
    user      User?    @relation(fields: [userId], references: [id])
}