model RssGenerator {
    id            String        @id @default(uuid())
    type          String
    website       String
    rssUrl        String?
    link          String?
    selector      String?
    userId        String?
    createdAt     DateTime      @default(now())
    updatedAt     DateTime?     @updatedAt
    lastExecuteAt DateTime?
    frequency     String?       @default("daily")
    tasks         ExecuteTask[]
    user          User?         @relation(fields: [userId], references: [id])
}
